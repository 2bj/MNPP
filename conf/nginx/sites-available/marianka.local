server {
    listen       80;
    server_name marianka.local;
    root   /Applications/MEPP/htdocs/marianka;
    
    access_log  /Applications/MEPP/logs/nginx/marianka.access.log;
    error_log  /Applications/MEPP/logs/nginx/marianka.error.log;

    index  index.html index.htm index.php;
    
    #error_page  404              /404.html;

    error_page   500 502 503 504  /50x.html;
    location = /50x.html {
        root   html;
    }

    include /Applications/MEPP/conf/nginx/common/magento;
 
}
